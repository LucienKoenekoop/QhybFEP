{NTA}                 ! Alanine
    [info]
        SYBYLtype  RESIDUE     !SYBYL substructure type
    [atoms]
         1 N       N3         -0.3000 !At. no., name, type, charge
         2 H1      H3          0.3300
         3 H2      H3          0.3300
         4 H3      H3          0.3300
         5 CA      CT          0.2500
         6 HA      HC          0.0600
         7 CB      CT         -0.1800
         8 HB1     HC          0.0600
         9 HB2     HC          0.0600
        10 HB3     HC          0.0600
        11 C       C           0.5000
        12 O       O          -0.5000
    [bonds]                !connect i -- j
       N    H1
       N    H2
       N    H3  
       N    CA
       CA   HA 
       CA   CB 
       CB   HB1
       CB   HB2
       CB   HB3
       CA   C  
       C    O  
    [connections]   !tail and head connections
        head     N  
        tail     C  
    [impropers]
        O    C    CA    +N
    [charge_groups] !charge groups, with switch atom first
        N    H1   H2   H3    CA    HA   
        CB   HB1  HB2   HB3
        C    O
*----------------------------------------------------------------------
